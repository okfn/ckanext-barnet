{
    "__options": {
        "append_unused": true,
        "blacklist": ["^creator_user_id$", "^maintainer$", "^maintainer_email$"]
    },
    "Delivery Unit": {
        "pattern_path": "^delivery_unit$",
        "unique": true
    },
    "Service": {
        "pattern_path": "^service$",
        "unique": true
    },
    "Data Owner": {
        "pattern_path": "^author$",
        "unique": true,
        "case_sensitive": true
    },
    "Contributor": {
        "pattern_path": ["^extras$", "^Contributor.*"]
    },
    "Title": {
        "pattern_path": "^title$",
        "unique": true,
        "case_sensitive": true
    },
    "Description": {
        "pattern_path": "^notes$",
        "unique": true,
        "case_sensitive": true,
        "max_length": 255
    },
    "Date Uploaded": {
        "pattern_path": "^metadata_created$",
        "unique": true
    },
    "Frequency": {
        "pattern_path": "^frequency$",
        "unique": true
    },
    "Retention Period Length": {
        "pattern_path": "^retention_count$",
        "unique": true
    },
    "Retention Period": {
        "pattern_path": "^retention_period",
        "unique": true
    },
    "Coverage Start Date": {
        "pattern_path": "^coverage_start_date$",
        "unique": true
    },
    "Coverage End Date": {
        "pattern_path": "^coverage_end_date$",
        "unique": true
    },
    "Format": {
        "pattern_path": ["^resources$", "^format$"],
        "case_sensitive": true,
        "deduplicate": true
    },
    "Source": {
        "pattern_path": "^url$",
        "unique": true,
        "case_sensitive": true
    },
    "Rights": {
        "pattern_path": "^license_title$",
        "unique": true,
        "case_sensitive": true
    },
    "Identifier": {
        "pattern_path": ["^extras$", "^Service Type URI.*"],
        "unique": true,
        "hyperlink": true
    },
    "Next Update": {
        "pattern_path": "^next_update$",
        "unique": true
    },
    "Version": {
        "pattern_path": "^version$",
        "unique": true,
        "case_sensitive": true
    },
    "Uploaded By": {
        "pattern_path": ["^creator_user_id$"],
        "unique": true,
        "case_sensitive": true
    },
    "Extras": {
        "pattern_path": ["^extras", ".*"],
        "return_multiple_columns": true
    }
}
